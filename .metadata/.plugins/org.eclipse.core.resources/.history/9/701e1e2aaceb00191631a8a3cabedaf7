#ifndef CLIENTE_H_
#define CLIENTE_H_

#define STATUS_EMPTY 0
#define STATUS_NOT_EMPTY 1
#define CANT_CARACTERES 51
#define CANT_REINTENTOS 2
#define CANT_CLIENTES 100

typedef struct
{
	int id;
	int status;
	//***************
	char nombre[51];
	char direccion[51];
	char cuit[51];
	char localidad[51];

}sCliente;

static int generarIdCliente(void);



#endif /* CLIENTE_H_ */
